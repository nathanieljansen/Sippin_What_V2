<div id="map"></div>
<div class="row">
  <div class="col s12 center">
    <h2 class="mainHeader">Love Us? Have a Pairing You Want to Share?</h2>
    <div>Let Us Know Your Thoughts</div>
  </div>
</div>
<div class="parallax-container pImage1">
  <div class="parallax">
    <img src="/images/wine-1476628_640.jpg">
  </div>
</div>
<div class="section white">
  <div class="row center container">

    <form class="col s12" id="contact-form">
      <div class="row">
        <div class="row">
          <div class="input-field col s12">
            <i class="material-icons prefix">account_circle</i>
            <input type="text" id="name" name="name" class="autocomplete-input">
            <label for="name">Name</label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s12">
            <i class="material-icons prefix">email</i>
            <input class="autocomplete-input" id="email" type="email" class="validate">
            <label for="email">Email</label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s12">
            <i class="material-icons prefix">edit</i>
            <textarea class="materialize-textarea" id="message"></textarea>
            <label for="textarea1">Send Us a Message</label>
            <p class "full">
              <button class="btn contactButton waves-effect waves-light" type="submit" name="action">Submit
              </button>
            </p>
            <div class="submitContact"></div>
          </div>
        </div>
    </form>
    </div>
  </div>
</div>
<div class="parallax-container">
  <div class="parallax">
    <img src="/images/cork-738603_640.jpg">
  </div>
</div>